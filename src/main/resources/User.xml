<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="aa">
    <select id="findById" resultType="com.zmate.pojo.User" parameterType="Integer">
       select * from user where id = #{id}
    </select>
    <select id="findByName" resultType="com.zmate.pojo.User" parameterType="String">
       select * from user where name like CONCAT ('%',#{name},'%')
    </select>
</mapper>
